#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() 
{
  float n1,n2;
  printf("Digite um número: ");
  scanf("%f",&n1);
  printf("Digite outro número:");
  scanf("%f",&n2);
  printf("\n");
  printf(" A soma dos números é: %5.2f", n1+n2);
  printf("\n A subtração dos números é: %5.2f", n1-n2);
  printf("\n A multiplicação dos números é: %5.2f", n1*n2);
  printf("\n A divisão dos números é: %5.2f", n1/n2);
  
  return 0;
}
